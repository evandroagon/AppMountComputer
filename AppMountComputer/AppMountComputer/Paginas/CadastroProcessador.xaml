<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppMountComputer.Paginas.CadastroProcessador"
             BackgroundImage="bg2.png"
             Title="Cadastro Peça">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="marca" Placeholder="marca" TextColor="#F29100" PlaceholderColor="Gray"/>
            <Entry x:Name="linha" Placeholder="linha" TextColor="#F29100" PlaceholderColor="Gray" />
            <Entry x:Name="modelo" Placeholder="modelo" TextColor="#F29100" PlaceholderColor="Gray" />
            <Entry x:Name="preco" Placeholder="preco" TextColor="#F29100" PlaceholderColor="Gray"/>

            <Button Text="Salvar" Clicked="salvarAction"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>